{
  "name": "@visualizevalue/opepicons",
  "version": "0.1.0",
  "description": "A tiny library for generating opepen blockies.",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visualizevalue/opepicons.git"
  },
  "author": "Seb Audet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/visualizevalue/opepicons/issues"
  },
  "homepage": "https://github.com/visualizevalue/opepicons#readme",
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.7.3"
  }
}
